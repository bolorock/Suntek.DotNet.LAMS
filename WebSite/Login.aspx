<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Login.aspx.cs" Inherits="WebSite.Login" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>登 陆</title>
</head>
<body style="height: 560px; font-family: Tahoma 宋体; font-weight: bold; font-size: 10pt;
    color: White; background: white url(Skins/Default/Images/login_bgline.jpg) repeat-x;">
    <div style="height: 30%">
    </div>
    <div style="height: 30%; text-align: center; vertical-align: middle;">
        <form id="form1" runat="server">
        <div style="margin: auto; width: 383px; height: 215px; background: url(Skins/Default/Images/loginbg.png) no-repeat;">
            <div style="height: 68px;">
            </div>
            <table style="margin: auto;">
                <tr>
                    <td colspan="2">
                        <asp:Label ID="lblMsg" runat="server" Font-Size="12px" ForeColor="Red"></asp:Label>
                    </td>
                </tr>
                <tr>
                    <td style="text-align: right; width: 30%;">
                        用户名：
                    </td>
                    <td style="text-align: left;">
                        <asp:TextBox ID="txtLoginName" Width="150px" runat="server"></asp:TextBox>
                    </td>
                </tr>
                <tr>
                    <td style="text-align: right; width: 30%;">
                        密码：
                    </td>
                    <td style="text-align: left;">
                        <asp:TextBox ID="txtPassword" Width="150px" runat="server" TextMode="Password"></asp:TextBox>
                    </td>
                </tr>
                <tr style="height: 10px">
                    <td>
                    </td>
                    <td>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <asp:ImageButton ImageUrl="Skins/Default/Images/login_btn.png" ID="btnSubmit" Width="80"
                            runat="server" Height="26" OnClick="btnSubmit_Click" />
                    </td>
                </tr>
            </table>
        </div>
        </form>
    </div>
    <div style="height: 20%">
    </div>
</body>
</html>
