.tag .goRight
{
    background-position: 100% 0px;
    float: right;
}
.tag .leftAct
{
    background-position: 0px -56px;
}
.tag .rightAct
{
    background-position: 100% -56px;
}
.tag .leftEnd
{
    background-position: 0px 20px;
}
.tag .rightEnd
{
    background-position: 100% 20px;
}
.tag .tagScorll
{
    left: 14px;
    overflow: hidden;
    bottom: -2px;
    position: absolute;
    height: 28px;
}
.tagScorll LI
{
    padding: 0 10px;
    display: inline;
    margin: 5px 0px 0px -1px;
    border-left: #ccc 1px solid;
    cursor: pointer;
    line-height: 15px;
    zoom: 1;
}

.hide
{
    display: none;
}
