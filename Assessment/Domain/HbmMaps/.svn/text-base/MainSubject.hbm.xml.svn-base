<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="SunTek.Assessment.Domain" assembly="SunTek.Assessment">
	<class name="SunTek.Assessment.Domain.MainSubject, SunTek.Assessment" table="LA_MainSubject" lazy="true">
		<id name="ID" column="ID">
			<generator class="assigned" />
		</id>
		<property name="Title" column="Title" />
		<property name="ExamPaperID" column="ExamPaperID" />
		<property name="Score" column="Score" />
		<property name="SortOrder" column="SortOrder" />
	</class>
</hibernate-mapping>