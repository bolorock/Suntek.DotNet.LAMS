using System;
using System.Collections.Generic;
using System.Text;

namespace Suntek.SAPService.Model
{
    public class WorkInfo
    {
        public WorkInfo() 
        {
            _begda = "";
            _endda = "";
            _zdw = "";
            _zzw = "";
        }

        private string _begda;
        private string _endda;
        private string _zdw;
        private string _zzw;

        /// <summary>
        /// 起始时间
        /// </summary>
        public string Begda
        {
            get
            {
                return this._begda;
            }
            set
            {
                this._begda = value;
            }
        }

        /// <summary>
        /// 结束时间
        /// </summary>
        public string Endda
        {
            get
            {
                return this._endda;
            }
            set
            {
                this._endda = value;
            }
        }

        /// <summary>
        /// 工作/学习地点
        /// </summary>
        public string Zdw
        {
            get
            {
                return this._zdw;
            }
            set
            {
                this._zdw = value;
            }
        }

        /// <summary>
        /// 岗位或职务
        /// </summary>
        public string Zzw
        {
            get
            {
                return this._zzw;
            }
            set
            {
                this._zzw = value;
            }
        }
    }
}
