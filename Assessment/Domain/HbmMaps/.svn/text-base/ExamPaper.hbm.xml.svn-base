<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="SunTek.Assessment.Domain" assembly="SunTek.Assessment">
	<class name="SunTek.Assessment.Domain.ExamPaper, SunTek.Assessment" table="LA_ExamPaper" lazy="true">
		<id name="ID" column="ID">
			<generator class="assigned" />
		</id>
		<property name="Title" column="Title" />
		<property name="Description" column="Description" />
		<property name="Category" column="Category" />
		<property name="DefaultScore" column="DefaultScore" />
		<property name="OwnerOrg" column="OwnerOrg" />
		<property name="Creator" column="Creator" />
		<property name="CreateTime" column="CreateTime" />
	</class>
</hibernate-mapping>